
page {
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: fixed;
  background-size: 100% 100%; 
  background-repeat:no-repeat;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}
